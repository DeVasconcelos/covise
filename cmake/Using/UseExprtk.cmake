MACRO(USE_EXPRTK)
    FIND_PACKAGE(EXPRTK)
    IF ((NOT EXPRTK_FOUND)  AND (${ARGC} LESS 1))
        USING_MESSAGE("Skipping because of missing EXPRTK")
        RETURN()
    ENDIF((NOT EXPRTK_FOUND) AND (${ARGC} LESS 1))
    IF(NOT EXPRTK_USED AND EXPRTK_FOUND)
        SET(EXPRTK_USED TRUE)
        INCLUDE_DIRECTORIES(SYSTEM ${EXPRTK_INCLUDE_DIR})
    ENDIF()
ENDMACRO(USE_EXPRTK)

